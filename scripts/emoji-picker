# Show emoji picker with rofi or fzf
if command -v rofi &>/dev/null; then
    # Use rofi
    selected=$(printf '%s\n' "${!EMOJIS[@]}" | rofi -dmenu -i -p "ðŸŽ­ Emoji" \
        -theme-str 'window { width: 800px; height: 600px; }' \
        -theme-str 'listview { lines: 15; }' \
        -theme-str 'element-icon { size: 24px; }')
    
    if [[ -n "$selected" ]]; then
        emoji="${EMOJIS[$selected]}"
        # Copy to clipboard
        if command -v wl-copy &>/dev/null; then
            echo -n "$emoji" | wl-copy
            notify-send "Emoji copied" "$emoji"
        elif command -v xclip &>/dev/null; then
            echo -n "$emoji" | xclip -selection clipboard
            notify-send "Emoji copied" "$emoji"
        elif command -v xsel &>/dev/null; then
            echo -n "$emoji" | xsel --clipboard --input
            notify-send "Emoji copied" "$emoji"
        else
            echo "$emoji"
        fi
    fi
else
    # Fallback to fzf
    selected=$(printf '%s\n' "${!EMOJIS[@]}" | fzf --prompt="Emoji: " --height=20)
    if [[ -n "$selected" ]]; then
        echo "${EMOJIS[$selected]}"
    fi
fi